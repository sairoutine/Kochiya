{
  "name": "Kochiya",
  "description": "Kochiya is a fork of Honoka which is a Bootstrap theme.",
  "keywords": [
    "css",
    "sass",
    "bootstrap",
    "japanese",
    "theme",
    "responsive",
    "front-end",
    "framework",
    "web",
    "touhou"
  ],
  "author": "sairoutine",
  "homepage": "http://sairoutine.github.io/Kochiya/",
  "repository": {
    "type": "git",
    "url": "git://github.com/sairoutine/Kochiya.git"
  },
  "license": "MIT",
  "moduleType": "globals",
  "main": [
    "dist/css/bootstrap.min.css"
  ],
  "ignore": [
    "/.*",
    "dist/assets",
    "dist/*.html",
    "src",
    "CNAME"
  ],
  "dependencies": {},
  "devDependencies": {
    "bootstrap": "3.3.6",
    "bootstrap-sass": "3.3.6"
  },
  "exportsOverride": {
    "bootstrap-sass": {},
    "bootstrap": {
      "js": "dist/js/bootstrap.**js",
      "fonts": "dist/fonts/**/*"
    },
    "jquery": {}
  }
}
